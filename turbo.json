{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"build-storybook": {
			"dependsOn": ["^build"],
			"outputs": ["storybook-static/**"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"test": {
			"dependsOn": ["^build"]
		},
		"dev": {
			"persistent": true,
			"cache": false,
			"dependsOn": ["^build"]
		}
	}
}
